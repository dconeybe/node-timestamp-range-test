{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "resolveJsonModule": true,
    "allowUnusedLabels": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "pretty": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "target": "es2020"
  }
}
